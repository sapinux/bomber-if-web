function scr_posicionar_jogador(player, player_id) {
	with (player) {
		switch (player_id) {
			case 1:
				x = 48
				y = 32
				break
			case 2:
				x = 256
				y = 224
				break
			case 3:
				x = 32
				y = 208
				break
			case 4:
				x = 288
				y = 64
				break
			case 5:
				x = 112
				y = 160
				break
			case 6:
				x = 208
				y = 96
				break
			case 7:
				x = 256
				y = 144
				break
			case 8:
				x = 64
				y = 112
				break
			case 9:
				x = 160
				y = 208
				break
			case 10:
				x = 160
				y = 48
				break
		}
	}
}     